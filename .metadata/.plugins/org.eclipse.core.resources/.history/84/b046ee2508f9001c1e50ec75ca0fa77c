package bubble.test.ex02;

import javax.swing.JFrame;

public class BubbleFrame extends JFrame {

	public BubbleFrame() {
		setSize(1000, 640);
		setLayout(null);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
	}

	public static void main(String[] args) {
		new BubbleFrame();
	}

}
